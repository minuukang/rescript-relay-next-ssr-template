{
  "name": "rescript-relay-next-ssr-template",
  "uncurried": true,
  "jsx": {
    "version": 4,
    "mode": "automatic"
  },
  "sources": {
    "dir": "src",
    "subdirs": true
  },
  "package-specs": [
    {
      "module": "es6",
      "in-source": true
    }
  ],
  "suffix": ".bs.js",
  "ppx-flags": ["rescript-relay/ppx"],
  "namespace": true,
  "bs-dependencies": [
    "@rescript/react",
    "rescript-relay"
  ]
}
